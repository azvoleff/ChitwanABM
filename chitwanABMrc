# Default values of parameters for the Chitwan Valley Non-Spatial Agent-based 
# Model. Values are read in to set defaults prior to initialization of the 
# model by the runModel script.
#
# Alex Zvoleff, aiz2101@columbia.edu

# Model-wide parameters
model.timezero : 1996 # The beginning of the model
model.endtime : 2020 # When the model stops
model.timestep : 1 # The size of each timestep
#    'model.RandomState' : [1], # Seeds the random number generator (useful for regenerating results later)
model.initial_num_persons : 5000
model.initial_num_households : 750
model.initial_num_neighborhoods : 65
model.resultspath : /home/azvoleff/chitwanABMresults
model.use_psyco : True

# Location of input data (these are restricted data)
input.census_file : /media/Restricted/CVABM_initialization_data/DS0004_export.csv
input.relationships_grid_file : /media/Restricted/CVABM_initialization_data/DS0016_export.csv
input.households_file : /media/Restricted/CVABM_initialization_data/DS0002_export.csv
input.neighborhoods_file : /media/Restricted/CVABM_initialization_data/DS0014_export.csv

# Person agent parameters
hazard_birth : 0, .03, .1, .2, .3, .6, .7, .8, .98, 1
hazard_death : .2, .03, .1, .2, .3, .6, .7, .8, .98, 1
hazard_marriage : 0, 0, .1, .2, .3, .6, .7, .8, .98, 1

# Household agent parameters
prob_any_non_wood_fuel : .5
prob_own_house_plot : .5
prob_own_any_land : .5
prob_rented_out_land : .5

# Neighborhood agent parameters
prob_elec : .1

# Landscape parameters
initial_proportion_veg : .3
initial_proportion_ag : .3
initial_proportion_private_infrastructure : .1
initial_proportion_public_infrastructure : .05
